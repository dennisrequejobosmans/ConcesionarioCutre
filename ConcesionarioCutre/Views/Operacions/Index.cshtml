@model IEnumerable<ConcesionarioCutre.Operacion>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PRECIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TIPO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Coch.MARCA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cliente.NIF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Empleado.NIF)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PRECIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TIPO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Coch.MARCA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente.NIF)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Empleado.NIF)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IDCOCHE, id1 = item.IDCLIENTE, id2 = item.IDEMPLEADO }) |
                @Html.ActionLink("Details", "Details", new { id = item.IDCOCHE, id1 = item.IDCLIENTE, id2 = item.IDEMPLEADO }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IDCOCHE, id1 = item.IDCLIENTE, id2 = item.IDEMPLEADO })
            </td>
        </tr>
    }

</table>
